<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ListPreferenceMultiSelect">
    	<attr format="string" name="checkAll" />
    	<attr format="string" name="separator" />
    </declare-styleable>
    
    <declare-styleable name="DeveloperPreference">
        <attr name="twitterHandle" format="string" />
        <attr name="nameDev" format="string"/>
        <attr name="donateLink" format="string" />
    </declare-styleable>

    <declare-styleable name="ImageListPreference">
        <attr name="summaryImage" format="reference" />
        <attr name="entryImages" format="reference" />
    </declare-styleable>

    <declare-styleable name="UrlSetting">
        <attr name="url" format="string"/>
    </declare-styleable>

    <declare-styleable name="CheckboxSetting">
        <attr name="descriptionOn" format="string"/>
        <attr name="descriptionOff" format="string"/>
    </declare-styleable>

    <declare-styleable name="ColorPickerSetting">
        <attr name="colorPickerDefaultValue" format="color"/>
        <attr name="colorPickerShowAlphaSlider" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="SeekBarPreference">
        <attr name="min" format="integer" />
        <attr name="interval" format="integer" />
        <attr name="unitsLeft" format="string|reference" />
        <attr name="unitsRight" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="indicatorColor" format="color" />
        <attr name="underlineColor" format="color" />
        <attr name="dividerColor" format="color" />
        <attr name="indicatorHeight" format="dimension" />
        <attr name="underlineHeight" format="dimension" />
        <attr name="dividerPadding" format="dimension" />
        <attr name="tabPaddingLeftRight" format="dimension" />
        <attr name="scrollOffset" format="dimension" />
        <attr name="tabBackground" format="reference" />
        <attr name="shouldExpand" format="boolean" />
        <attr name="textAllCaps" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AppSelectListPreference">
        <attr name="neutralButtonText" format="string|reference" />
        <attr name="neutralButtonValue" format="string|reference" />
        <attr name="positiveButtonValue" format="string|reference" />
    </declare-styleable>

</resources>
